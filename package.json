{
  "name": "@radatek/microdb",
  "version": "1.0.2",
  "description": "Lightweight plain file store database with MongoDB interface",
  "author": "Darius Kisonas",
  "license": "MIT",
  "keywords": [
    "nosql",
    "embedded",
    "database",
    "microdb",
    "mongodb"
  ],
  "files": [
    "./dist/microdb.js",
    "./dist/microdb.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/microdb.js",
      "require": "./dist/microdb.cjs"
    }
  },
  "type": "module",
  "main": "./dist/microdb.js",
  "module": "./dist/microdb.js",
  "types": "./dist/microdb.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/radteki/microdb.git"
  },
  "scripts": {
    "build": "pnpm --package typescript dlx tsc -d -t es2017 microdb.ts --outDir dist"
  },
  "devDependencies": {
    "@types/node": "^22.14.0"
  }
}
